= content_for :title, "Degust - sessions"

.container

  .row
    .col-md-12
      .page-header
        %h1 Your Data

  - if !current_user.nil?
    .row
      .col-md-12
        You are logged in as
        %b #{current_user.name}
        via
        %b #{if current_user.provider == "github" then "github" else "google" end}

  .row
    .col-md-12
      .mine
        - if @mine.length
          %h4 My Data sets
          = render :partial => "table", :locals => { :list => @mine }
      .visited
        - if @others.length
          %h4 Data sets shared with me
          = render :partial => "table", :locals => { :list => @others }
